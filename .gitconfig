[user]
	email = minami.polly@gmail.com
	name = Naoto Minami
[ghq]
	root = ~/src
[core]
	editor = nvim
[alias]
	pr = pull-request
	open = browse
[rebase]
    autostash = true
[pull]
	rebase = false
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
